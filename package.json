{
  "name": "blog-vue",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "heroku-postbuild": "npm run build",
    "lint": "npm run lint:js",
    "test": "jest",
    "watch:sass": "node-sass assets/scss/main.scss assets/css/style.css -w",
    "compile:sass": "node-sass assets/scss/main.scss assets/css/style.comp.css",
    "concat:css": "concat -o assets/css/style.concat.css assets/css/style.comp.css",
    "compress:css": "node-sass assets/css/style.comp.css assets/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css compress:css"
  },
  "dependencies": {
    "@johmun/vue-tags-input": "^2.1.0",
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/toast": "^3.3.1",
    "@nuxtjs/vuetify": "^1.11.3",
    "concat": "^1.0.3",
    "cors": "^2.8.5",
    "javascript-time-ago": "^2.3.4",
    "nuxt": "^2.15.4",
    "vue-awesome": "^4.1.0",
    "vue-datepicker-ui": "^2.3.0",
    "vue-multiselect": "^2.1.6",
    "vue-notification": "^1.3.20",
    "vue-pdf": "^4.2.0",
    "vue-social-sharing": "^3.0.7",
    "vue2-editor": "^2.10.2"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.4",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@vue/test-utils": "^1.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.2.1",
    "vue-jest": "^3.0.7"
  }
}
